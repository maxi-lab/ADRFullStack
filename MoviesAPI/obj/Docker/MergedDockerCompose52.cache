name: dockercompose2751183814696163273
services:
  database:
    container_name: database
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: YourStrong@Passw0rd123
    healthcheck:
      test:
        - CMD-SHELL
        - /opt/mssql-tools18/bin/sqlcmd -S localhost -U SA -P "YourStrong@Passw0rd123" -Q "SELECT 1"
      timeout: 3s
      interval: 10s
      retries: 10
      start_period: 10s
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: bind
        source: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI\sqlserver\data
        target: /var/opt/mssql/data
        bind:
          create_host_path: true
      - type: bind
        source: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI\sqlserver\log
        target: /var/opt/mssql/log
        bind:
          create_host_path: true
  moviesapi:
    build:
      context: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI
      dockerfile: MoviesAPI/Dockerfile
      args:
        BUILD_CONFIGURATION: Debug
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MoviesAPI
      target: base
    container_name: moviesapi
    depends_on:
      database:
        condition: service_healthy
        required: true
    entrypoint:
      - dotnet
      - --roll-forward
      - Major
      - /VSTools/DistrolessHelper/DistrolessHelper.dll
      - --wait
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "5080"
      ASPNETCORE_HTTPS_PORTS: "5081"
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: moviesapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages  "/app/bin/Debug/net8.0/MoviesAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: dotnet --roll-forward Major /VSTools/DistrolessHelper/DistrolessHelper.dll --stop dotnet
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5000
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 5001
        published: "5001"
        protocol: tcp
      - mode: ingress
        target: 5080
        protocol: tcp
      - mode: ingress
        target: 5081
        protocol: tcp
    tty: true
    volumes:
      - type: bind
        source: C:\Users\Maximo\AppData\Roaming\Microsoft\UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Maximo\AppData\Roaming\ASP.NET\Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI\MoviesAPI
        target: /app
        bind:
          create_host_path: true
      - type: bind
        source: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI
        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Maximo\vsdbg\vs2017u5
        target: /remote_debugger
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Maximo\.nuget\packages
        target: /.nuget/packages
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Maximo\AppData\Roaming\ASP.NET\Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Maximo\AppData\Roaming\Microsoft\UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
