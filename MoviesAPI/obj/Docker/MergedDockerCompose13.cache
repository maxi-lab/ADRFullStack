name: dockercompose2751183814696163273
services:
  database:
    container_name: database
    environment:
      ACCEPT_EULA: "y"
      SA_PASSWORD: YourStrong
    image: mcr.microsoft.com/mssql/server
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: bind
        source: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI\sqlserver\data
        target: /var/opt/msssql/data
        bind:
          create_host_path: true
      - type: bind
        source: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI\sqlserver\log
        target: /var/opt/msssql/log
        bind:
          create_host_path: true
  moviesapi:
    build:
      context: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI
      dockerfile: MoviesAPI/Dockerfile
    container_name: moviesapi
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "5080"
      ASPNETCORE_HTTPS_PORTS: "5081"
    image: moviesapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5000
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 5001
        published: "5001"
        protocol: tcp
      - mode: ingress
        target: 5080
        protocol: tcp
      - mode: ingress
        target: 5081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Maximo\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Maximo\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose2751183814696163273_default