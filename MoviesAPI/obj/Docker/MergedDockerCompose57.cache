name: dockercompose2751183814696163273
services:
  database:
    container_name: database
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: YourStrong@Passw0rd123
    healthcheck:
      test:
        - CMD-SHELL
        - /opt/mssql-tools18/bin/sqlcmd -S localhost -U SA -P "YourStrong@Passw0rd123" -Q "SELECT 1" -No
      timeout: 3s
      interval: 10s
      retries: 10
      start_period: 10s
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: bind
        source: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI\sqlserver\data
        target: /var/opt/mssql/data
        bind:
          create_host_path: true
      - type: bind
        source: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI\sqlserver\log
        target: /var/opt/mssql/log
        bind:
          create_host_path: true
  frontend:
    build:
      context: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI\.MoviesAPI\Client\frontend
      dockerfile: Dockerfile
    container_name: front
    image: front
    networks:
      default: null
    ports:
      - mode: ingress
        target: 3000
        published: "3000"
        protocol: tcp
  moviesapi:
    build:
      context: D:\UTN\Administracion de sistemas de informacion\TP5\MoviesAPI
      dockerfile: MoviesAPI/Dockerfile
    container_name: moviesapi
    depends_on:
      database:
        condition: service_healthy
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "5080"
      ASPNETCORE_HTTPS_PORTS: "5081"
    image: moviesapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5000
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 5001
        published: "5001"
        protocol: tcp
      - mode: ingress
        target: 5080
        protocol: tcp
      - mode: ingress
        target: 5081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Maximo\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Maximo\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose2751183814696163273_default